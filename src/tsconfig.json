{
    "compilerOptions": {
        "target": "es6",
        "declaration": false,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "atom": {
        "rewriteTsconfig": false
    }
}
